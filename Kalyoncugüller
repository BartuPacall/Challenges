#include <stdio.h>
void main()
{
    int array[8][6]={{2,2},{3,3,3,3},{4},{5,5,5},{6,6,6,6,6},{7,7},{8,8,8,8},{9,9,9,9,9,9}};
    for (int i=0;i<8;i++){
        for(int j=0;j<6;j++){
            if(array[i][j]!=0){
                printf("%d ",array[i][j]);
            }
        }
        printf("\n");
    }
}
